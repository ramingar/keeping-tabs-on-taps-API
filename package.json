{
  "name": "keeping-tabs-on-taps",
  "version": "0.1.0",
  "description": "An awesome counter for your beers debt",
  "main": "./dist/index.js",
  "scripts": {
    "dev": "NODE_ENV=development ./node_modules/.bin/nodemon -w src --exec \"./node_modules/.bin/babel-node src --presets env\"",
    "prestart": "npm run -s build",
    "build": "./node_modules/.bin/babel src -s -D -d dist --presets env",
    "start": "NODE_ENV=production node ./dist/",
    "test": "export BABEL_ENV=test && ./node_modules/.bin/babel-tape-runner ./tests/*.test.js | ./node_modules/.bin/faucet"
  },
  "author": "Rafael Minguet Garc√≠a <rminguet@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.18.1",
    "express": "^4.15.4",
    "fs": "0.0.1-security",
    "https": "^1.0.0",
    "mongoose": "^4.11.11"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-tape-runner": "^2.0.1",
    "faucet": "0.0.1",
    "nodemon": "^1.12.1",
    "supertest": "^3.0.0",
    "tape": "^4.8.0"
  }
}
